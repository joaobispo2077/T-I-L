{
  "all": true,
  "instrument": true,
  "extension": [
    "js"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "exclude": [
    "coverage",
    "server/**",
    "config/**",
    "tests/**/*",
    "**/*.spec.js",
    "lcov-report/**/*",
    "scripts"
  ],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
